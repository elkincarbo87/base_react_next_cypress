{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "@stylexjs"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "error",
    "@stylexjs/valid-styles": "error",
    "quotes": ["error", "single"],
    "no-console": "error"
  }
}
